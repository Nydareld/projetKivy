<Game>:
    canvas:
        Color:
            rgba: 0.1,0.1,0.1,1


<Bloc>:
    y: self.position*self.game.offset+self.game.startx
    x: self.positionLaterale

    canvas:
        Color:
            rgba: root.rgba
        Rectangle:
            pos: (self.pos[0],self.pos[1])
            size: self.size
